.popup {
    position: relative;
    padding-left: 10%;
    padding-right: 10%;
    padding-top: 10%;
    padding-bottom: 10%;
    flex-grow: 1;
}

.popup_text {
    -unity-font-definition: none;
    font-size: 32px;
    -unity-text-align: upper-left;
    white-space: normal;
}

.window {
    position: absolute;
    width: 100%;
    height: 100%;
    background-color: rgb(255, 255, 255);
}
